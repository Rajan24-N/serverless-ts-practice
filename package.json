{
  "name": "serverless-ts-template",
  "version": "0.0.0-development",
  "description": "Serverless Typescript template",
  "scripts": {
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile",
    "lint": "eslint .",
    "prettier": "prettier --list-different './**/*.{js,ts}'",
    "type-check": "tsc --noEmit",
    "package": "sls package",
    "deploy": "sls deploy"
  },
  "engines": {
    "node": ">=18.20.4"
  },
  "dependencies": {
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "serverless-offline": "^11.6.0",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-schema": "^1.18.0",
    "serverless-step-functions": "^3.11.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.8.0",
    "serverless": "^4.3.3",
    "serverless-esbuild": "^1.52.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.1.3",
    "typescript-eslint": "^8.6.0"
  },
  "author": "Distinction Dev",
  "license": "MIT"
}
